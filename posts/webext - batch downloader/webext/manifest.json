{
    "manifest_version": 2,
    "name": "Batch downloader",
    "version": "1.0.0",
    "description": "Download many links easily.",
    "author": "lockex1987",
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "Batch downloader"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "notifications",
        "downloads",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ]
}