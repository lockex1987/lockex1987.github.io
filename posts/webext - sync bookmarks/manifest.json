{
    "name": "Sync bookmarks",
    "description": "Push and pull bookmarks between local and cloud.",
    "version": "1.0.0",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "default_title": "Sync bookmarks",
        "default_icon": "images/icon-16.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://lockex1987.github.io; object-src 'self'"
}